{
  "compilerOptions": {
    "target": "es2016",

    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["node"],

    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
  },
}